{
  "paths": {
    "/process-instances/{key}": {
      "delete": {
        "summary": "Delete process instance and dependant data by key",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/ProcessInstanceDeleteResponse" }
              }
            }
          },
          "400": {
            "description": "Invalid request",
            "content": {
              "application/problem+json": {
                "schema": { "$ref": "#/components/schemas/Problem" }
              }
            }
          },
          "403": {
            "description": "Forbidden",
            "content": {
              "application/problem+json": {
                "schema": { "$ref": "#/components/schemas/Problem" }
              }
            }
          },
          "404": {
            "description": "Not Found",
            "content": {
              "application/problem+json": {
                "schema": { "$ref": "#/components/schemas/Problem" }
              }
            }
          },
          "500": {
            "description": "API application error",
            "content": {
              "application/problem+json": {
                "schema": { "$ref": "#/components/schemas/Problem" }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ProcessInstanceDeleteResponse": {
        "type": "object",
        "properties": {
          "message": { "type": "string" },
          "deleted": { "type": "integer", "format": "int64" }
        },
        "required": ["message", "deleted"]
      }
    }
  }
}
